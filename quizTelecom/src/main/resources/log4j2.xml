<?xml version="1.0" encoding="UTF-8"?>

<Configuration package="fr.telecom_st_etienne.contrat" status="INFO">
        <Appenders>
                <File name="File" fileName="log/contrat_log">
                        <PatternLayout>
                                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
                        </PatternLayout>
                </File>
        </Appenders>
        <Loggers>
                <Root level="info">
                        <AppenderRef ref="File" />
                </Root>
        </Loggers>
</Configuration>